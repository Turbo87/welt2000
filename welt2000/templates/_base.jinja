<html>
  <head>
    <title>WELT2000</title>
  </head>
  <body>
    <h1>WELT2000</h1>

    <p>
    <a href="{{ url_for('index') }}">[{{ _('Home') }}]</a>
    <a href="{{ url_for('not_available') }}">[{{ _('News') }}]</a>
    <a href="{{ url_for('contribute') }}">[{{ _('Contribute') }}]</a>
    <a href="{{ url_for('not_available') }}">[{{ _('Downloads') }}]</a>
    <a href="{{ url_for('references') }}">[{{ _('References') }}]</a>
    <a href="{{ url_for('not_available') }}">[{{ _('FAQ') }}]</a>
    </p>

    {% block content %}{% endblock %}

    <p>
    <a href="{{ url_for('contact') }}">[{{ _('Contact') }}]</a>
    <a href="{{ url_for('not_available') }}">[{{ _('Legal Terms') }}]</a>
    <a href="{{ url_for('not_available') }}">[{{ _('RSS') }}]</a>
    <a href="{{ url_for('about') }}">[{{ _('About') }}]</a>

    {% if get_locale() == 'de' -%}
    <a href="{{ url_for('lang', lang='en') }}"><img src="{{ url_for('static', filename='flags/gb.png') }}"></a>
    {%- else -%}
    <a href="{{ url_for('lang', lang='de') }}"><img src="{{ url_for('static', filename='flags/de.png') }}"></a>
    {%- endif %}
    </p>
  </body>
</html>
